{
  "/": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Homepage is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/category/:category": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Category pages are public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/product/:id": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Product detail pages are public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/cart": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "User must be logged in to view cart",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login"
    }
  },
  "/checkout": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "User must be logged in to checkout",
            "rule": "authenticated"
          }
